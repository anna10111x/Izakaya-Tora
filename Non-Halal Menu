    private void S5ActionPerformed(java.awt.event.ActionEvent evt) {                                   
        String portion = (String) jComboBox6.getSelectedItem(); // get Small, Regular, etc.
        int basePrice = 28;
        int price;

         switch (portion) {
            case "Small" -> price = basePrice;
            case "Regular" -> price = basePrice * 1;
            case "Large" -> price = basePrice * 2;
            default -> price = basePrice;}
        FoodItem item = new FoodItem(1, "ST Majboos",1, price ,portion);
        cartManager.addItem(item);
        addtable(item);
        updateTotal();
    }                                  

