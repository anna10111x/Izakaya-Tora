    private void S7ActionPerformed(java.awt.event.ActionEvent evt) {                                   
        String portion = (String) jComboBox1.getSelectedItem(); // get Small, Regular, etc.
            int basePrice = 28;
            int price;

         switch (portion) {
            case "Small" -> price = basePrice;
            case "Regular" -> price = basePrice * 1;
            case "Large" -> price = basePrice * 2;
            default -> price = basePrice;}
        FoodItem item = new FoodItem(1, "YSQ Mandi", 1, price ,portion);
        cartManager.addItem(item);
        addtable(item);
        updateTotal();
    }                                  

    private void jButton32ActionPerformed(java.awt.event.ActionEvent evt) {                                          
        int selectedRow = jTable2.getSelectedRow();
        
        if (selectedRow >= 0){
            int id = (int)jTable2.getValueAt(selectedRow, 0);
            cartManager.removeItemById(id);
            
            ((DefaultTableModel)jTable2.getModel()).removeRow(selectedRow);
            updateTotal();
            
            JOptionPane.showMessageDialog(this, "Item has been removed from Cart.");
        } else{
            JOptionPane.showMessageDialog(this, "Please select an item to remove.");
        }
    }                                         

    private void S1ActionPerformed(java.awt.event.ActionEvent evt) {                                   
            String portion = (String) jComboBox2.getSelectedItem(); // get Small, Regular, etc.
            int basePrice = 35;
            int price;

         switch (portion) {
            case "Small" -> price = basePrice;
            case "Regular" -> price = basePrice * 1;
            case "Large" -> price = basePrice * 2;
            default -> price = basePrice;}
        FoodItem item = new FoodItem(1, "CS Camel Bao",1, price ,portion);
        cartManager.addItem(item);
        addtable(item);
        updateTotal();
    }                                  

